
public class Main {

	public static void main(String[] args) {
		int sayi1 =20;
		int sayi2 =35;
		int sayi3 =2;
		
		if(sayi2<sayi1) {
		if(sayi3<sayi1) {
			System.out.println(sayi1);
		}
		}
		else if(sayi2>sayi1) {
		if(sayi3<sayi2) {
				System.out.println(sayi2);
		}
		}
		else {
		System.out.println(sayi3);
		}
}
}